<h1>This is the show page of lists</h1>
<h2><%= @list.name %></h2>
  <% @list.movies.each do |movie| %>
    <%= movie.title %>
  <%= end %>


<div>
  <button type="button"><%= link_to "Back to lists", lists_path %></button>
  <button type="button"><%= link_to "Add a bookmark", new_list_bookmark_path(@list) %></button>
</div>
